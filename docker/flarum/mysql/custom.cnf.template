# Flarum MariaDB Configuration - Generated from triforce.env

[mysqld]
innodb_buffer_pool_size = ${FLARUM_MYSQL_BUFFER_POOL}
max_connections = ${FLARUM_MYSQL_MAX_CONNECTIONS}
max_allowed_packet = ${FLARUM_MYSQL_MAX_PACKET}
character-set-server = utf8mb4
collation-server = utf8mb4_unicode_ci

[client]
default-character-set = utf8mb4
